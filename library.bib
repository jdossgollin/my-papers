@article{kwakkel:2015,
  title = {Developing Dynamic Adaptive Policy Pathways: A Computer-Assisted Approach for Developing Adaptive Strategies for a Deeply Uncertain World},
  shorttitle = {Developing Dynamic Adaptive Policy Pathways},
  author = {Kwakkel, Jan H. and Haasnoot, Marjolijn and Walker, Warren E.},
  date = {2015-10-01},
  journaltitle = {Climatic Change},
  shortjournal = {Climatic Change},
